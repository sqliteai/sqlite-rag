{
    "name": "Python 3.10",
    "image": "mcr.microsoft.com/devcontainers/python:3.10",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.black-formatter",
                "ms-python.flake8",
                "ms-python.isort",
                "ms-python.vscode-pylance",
                "ms-python.python",
                "ms-python.debugpy",
                "ms-python.vscode-python-envs",
                "hbenl.vscode-test-explorer"
            ]
        }
    },
    "runArgs": ["--network=host"]
}
